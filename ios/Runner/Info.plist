<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Daily Quran</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>daily_quran</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>com.hexahelix.dq</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>DailyQuran</string>
			</array>
		</dict>
		<dict>
			<key>CFBundleURLName</key>
			<string>Google Sign-In</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.googleusercontent.apps.134970392054-86d1gomong6gdbdtu6c62p4knpouqh02</string>
			</array>
		</dict>
	</array>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>FirebaseAppDelegateProxyEnabled</key>
	<true/>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSCameraUsageDescription</key>
	<string>Daily Quran uses the camera to take photos for your profile picture and to capture images when creating content.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Daily Quran uses your location to calculate the Qiblah direction so you can face the correct direction toward Mecca during prayer.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Daily Quran uses your location to show the Qiblah direction and update it as you move, even when the app is in the background.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Daily Quran accesses your photo library to select images for your profile picture and to upload content.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Daily Quran saves downloaded verse images and progress screenshots to your Photos app so you can access them later.</string>
	<key>NSUserNotificationUsageDescription</key>
	<string>Daily Quran sends notifications to alert you about live events and important updates.</string>
	<key>NSUserNotificationsUsageDescription</key>
	<string>Daily Quran sends notifications to alert you about prayer times and to play the adhan call to prayer.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Daily Quran uses the microphone to record your Quran recitation for practice and to create voice notes to share with your teacher.</string>
	<key>NSAppleMusicUsageDescription</key>
	<string>Daily Quran plays the adhan call to prayer audio at scheduled prayer times.</string>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>UIBackgroundModes</key>
	<array>
		<string>fetch</string>
		<string>remote-notification</string>
		<string>audio</string>
	</array>
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>googlegmail</string>
		<string>googlemail</string>
		<string>googleplus</string>
		<string>googledrive</string>
		<string>googlemaps</string>
		<string>googleearth</string>
		<string>googlechrome</string>
		<string>googlechromes</string>
		<string>googleapp</string>
		<string>youtube</string>
		<string>yt</string>
		<string>comgooglemaps</string>
		<string>comgoogleearth</string>
		<string>comgoogleplus</string>
		<string>comgooglegmail</string>
		<string>comgoogledrive</string>
		<string>comgooglechrome</string>
		<string>comgoogleapp</string>
		<string>comyoutube</string>
		<string>comyt</string>
	</array>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
		<key>NSExceptionDomains</key>
		<dict>
			<key>googleapis.com</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
				<key>NSExceptionMinimumTLSVersion</key>
				<string>TLSv1.0</string>
				<key>NSIncludesSubdomains</key>
				<true/>
			</dict>
			<key>googleusercontent.com</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
				<key>NSExceptionMinimumTLSVersion</key>
				<string>TLSv1.0</string>
				<key>NSIncludesSubdomains</key>
				<true/>
			</dict>
		</dict>
	</dict>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
	</array>
</dict>
</plist>
